.Header
	background-color black
	color grey-clear
	padding-bottom 6px
	padding-left .5rem
	padding-right .5rem
	&-top
		display flex
		justify-content space-between
		align-items: center
	&-navResponse
		position absolute
		right -100%
		top 65px
		transition .5s
		width 100%
		text-align center
	&-label2
		position absolute
		right 0
	&-menu
		.check	
			display none		
			&:checked~
				.Header-navResponse
					position absolute
					right 0%
				    transition .5s	
				    z-index 3